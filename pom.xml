<?xml version="1.0" encoding="utf-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.veeva.vault.custom</groupId>
    <artifactId>vault-object-record-sdk-example</artifactId>
    <version>1.0-SNAPSHOT</version>
    <properties>
        <vault.sdk.version>[22.1.2-release0, 22.1.2-release1000000]</vault.sdk.version>
    </properties>
    <repositories>
        <repository>
            <id>veevavault</id>
            <url>https://repo.veevavault.com/maven</url>
            <releases>
                <enabled>true</enabled>
                <updatePolicy>always</updatePolicy>
            </releases>
        </repository>
    </repositories>
    <pluginRepositories>
        <pluginRepository>
            <id>vaultjavasdk-maven-plugin</id>
            <url>https://veeva.github.io/vaultjavasdk-maven-plugin/maven</url>
            <releases>
                <enabled>true</enabled>
                <updatePolicy>always</updatePolicy>
            </releases>
        </pluginRepository>
    </pluginRepositories>
    <dependencies>
        <dependency>
            <groupId>com.veeva.vault.sdk</groupId>
            <artifactId>vault-sdk</artifactId>
            <version>${vault.sdk.version}</version>
        </dependency>
    </dependencies>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.8.1</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>com.veeva.vault.sdk</groupId>
                <artifactId>vaultjavasdk-maven-plugin</artifactId>
                <version>22.2.0</version>
                <configuration>
                    <vaultDNS></vaultDNS>
                    <username></username>
                    <password></password>
                    <deploymentOption>replace_all</deploymentOption>
                    <source>
                        <packages></packages>
                        <classes></classes>
                    </source>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>